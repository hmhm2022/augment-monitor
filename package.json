{
  "name": "augment-monitor",
  "displayName": "Augment Monitor",
  "description": "Monitor your Augment usage and subscription status",
  "version": "0.1.0",
  "publisher": "augment-code",
  "repository": {
    "type": "git",
    "url": "https://github.com/augment-code/augment-monitor"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "augment-monitor.showUsage",
        "title": "Show Augment Usage"
      },
      {
        "command": "augment-monitor.setToken",
        "title": "Set Augment Token"
      }
    ],
    "configuration": {
      "title": "Augment Monitor",
      "properties": {
        "augment-monitor.token": {
          "type": "string",
          "default": "",
          "description": "Your Augment token"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "lint": "eslint src --ext ts",
    "vsce:package": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.0",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vscode/vsce": "^2.22.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "axios": "^0.24.0"
  }
}
